using System;

namespace BAEKJOON_11022
{
    class Program
    {
        static void Main(string[] args)
        {
            int T = int.Parse(Console.ReadLine());

            for(int i = 0; i < T; i++)
            {
                string[] arr = Console.ReadLine().Split(' ');
                int A = int.Parse(arr[0]);
                int B = int.Parse(arr[1]);
                Console.WriteLine($"Case #{i+1}: {A} + {B} = {A+B}");
            }
        }
    }
}
