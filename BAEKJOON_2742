using System;
using System.Text;

namespace BAEKJOON_2742
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = int.Parse(Console.ReadLine());
            StringBuilder sb = new StringBuilder();

            for(int i = N; i > 0; i--)
            {
                sb.Append(i+"\n");
            }
            Console.WriteLine(sb);
        }
    }
}
