using System;

namespace BAEKJOON_10950
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = int.Parse(Console.ReadLine());

            

            for (int i = 0; i < N; i++)
            {
                string[] arr = Console.ReadLine().Split(' ');

                int A = int.Parse(arr[0]);
                int B = int.Parse(arr[1]);

                Console.WriteLine(A+B);
            }
        }
    }
}
