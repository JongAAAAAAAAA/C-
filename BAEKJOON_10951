using System;

namespace BAEKJOON_10951
{
    class Program
    {
        static void Main(string[] args)
        {
            while (true)
            {
                string[] arr = Console.ReadLine().Split(' ');
                if (arr == null)
                    break;
                int A = int.Parse(arr[0]);
                int B = int.Parse(arr[1]);

                Console.WriteLine(A+B);
            }
        }
    }
}
